{
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": "off",
        "indent": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-debugger": "off",
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ]
    }
}
